{
  "env": {
    "jest": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "standard",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": [
      1,
      {
        "allow": ["info", "warn", "error"]
      }
    ],
    "eol-last": ["error", "always"],
    "react/prop-types": 0
  }
}
